generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Contact {
  id        String   @id @default(cuid())
  name      String
  email     String
  message   String
  completed Boolean  @default(false)
  createdAt DateTime @default(now())
}

model Admin {
  id       String @id @default(cuid())
  username String @unique
  password String
}

model PageView {
  id        String   @id @default(cuid())
  page      String
  createdAt DateTime @default(now())
}

